/Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><Description>History of Remediations</Description><Author>Mattias Benninge</Author><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>default_reference</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>347b5f16-b635-4613-a324-70d8ab1eb764</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="LCID"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@locale"><Value>=Parameters!locale.Value</Value></QueryParameter></QueryParameters><CommandText>select dbo.fn_LShortNameToLCID(@locale)</CommandText></Query><Fields><Field Name="ID"><DataField /><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="param_ListCIs"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@lcid"><Value>=Parameters!LCID.Value</Value></QueryParameter><QueryParameter Name="@UserSIDs"><Value>=Parameters!UserSIDs.Value</Value></QueryParameter></QueryParameters><CommandText>                select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName
					,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
					,ci.CI_ID
                from fn_rbac_ConfigurationItems(@UserSIDs)  ci  
                inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
                inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
                where ci.CIType_ID not in (2,11,14) and ci.CI_ID in (Select Distinct(CI_ID) from v_CIRemediationHistory)

            order by CIDisplayName</CommandText></Query><Fields><Field Name="CIDisplayName"><DataField>CIDisplayName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CIName"><DataField>CIName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CI_ID"><DataField>CI_ID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="DataSetAdminID"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@UserTokenSIDs"><Value>=Parameters!UserTokenSIDs.Value</Value></QueryParameter></QueryParameters><CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText></Query><Fields><Field Name="UserSIDs"><DataField>UserSIDs</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="main"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@LCID"><Value>=Parameters!LCID.Value</Value></QueryParameter><QueryParameter Name="@CI_CIID"><Value>=Parameters!CI_CIID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT CIR.[CIVersion]
      ,CIR.[SDMPackageVersion]
      ,sys.Netbios_Name0
      ,[UserName]
      ,[FullName]
      ,[UserID]
      ,[RemediationDate]
      ,[ComplianceState]
      ,dbo.fn_GetLocalizedCIName(@LCID,cir.CI_ID) as CIName
      ,ResourceID_count as NumberOfRemediations

  FROM v_CIRemediationHistory CIR
  Join v_R_System sys on cir.ResourceID = sys.ResourceID
  join (select ResourceID, count(*) as ResourceID_count
          from v_CIRemediationHistory
		  where CI_ID = @CI_CIID
         group by ResourceID) b
    on CIR.ResourceID = b.ResourceID
  where cir.CI_ID = @CI_CIID</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="CIVersion"><DataField>CIVersion</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="SDMPackageVersion"><DataField>SDMPackageVersion</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Netbios_Name0"><DataField>Netbios_Name0</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UserName"><DataField>UserName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UserID"><DataField>UserID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="RemediationDate"><DataField>RemediationDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="ComplianceState"><DataField>ComplianceState</DataField><rd:TypeName>System.Byte</rd:TypeName></Field><Field Name="CIName"><DataField>CIName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="NumberOfRemediations"><DataField>NumberOfRemediations</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="UserTokenSIDs"><DataType>String</DataType><DefaultValue><Values><Value>=SrsResources.UserIdentity.GetUserSIDs(User!UserID)</Value></Values></DefaultValue><Hidden>true</Hidden></ReportParameter><ReportParameter Name="UserSIDs"><DataType>String</DataType><DefaultValue><DataSetReference><DataSetName>DataSetAdminID</DataSetName><ValueField>UserSIDs</ValueField></DataSetReference></DefaultValue><Hidden>true</Hidden></ReportParameter><ReportParameter Name="locale"><DataType>String</DataType><DefaultValue><Values><Value>=User!Language</Value></Values></DefaultValue><Hidden>true</Hidden></ReportParameter><ReportParameter Name="LCID"><DataType>Integer</DataType><DefaultValue><DataSetReference><DataSetName>LCID</DataSetName><ValueField>ID</ValueField></DataSetReference></DefaultValue><Prompt>LCID</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="CI_CIID"><DataType>String</DataType><Prompt>Configuration Item</Prompt><ValidValues><DataSetReference><DataSetName>param_ListCIs</DataSetName><ValueField>CI_ID</ValueField><LabelField>CIDisplayName</LabelField></DataSetReference></ValidValues></ReportParameter></ReportParameters><CodeModules><CodeModule>SrsResources, Version=5.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35</CodeModule></CodeModules><rd:ReportUnitType>Mm</rd:ReportUnitType><rd:ReportID>b78a5471-ab85-4c57-9dda-46c2e5fa02e1</rd:ReportID></Report>